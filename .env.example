# IBKR Gateway Configuration

IBKR_TRADING_MODE=paper

IBKR_USERID=
IBKR_PASSWORD=
IBKR_GATEWAY_LIVE_PORT=4003

IBKR_USERID_PAPER=
IBKR_PASSWORD_PAPER=
IBKR_GATEWAY_PAPER_PORT=4004

# MCP Server Configuration
# Server port for HTTP/SSE endpoint (default: 8080)
# Access the MCP server at: http://127.0.0.1:8080/mcp
SERVER_PORT=8080

# Read-only mode: Disable order placement, modification, and cancellation
# Set to true/1/yes to enable read-only mode
READONLY=true

# Optional: VNC password for monitoring the gateway UI
VNC_PASSWORD=12345678
